@model ComponentViewModel<GenericColumnSectionProperties>

@{
    int.TryParse(Model.Properties.ColumnCount, out int colCount);
}

<div class="generic-column-section">
    @for (int i = 0; i < colCount; i++)
    {
        <div class="generic-col-@(12 / colCount)">
            <widget-zone />
        </div>
    }
</div>
